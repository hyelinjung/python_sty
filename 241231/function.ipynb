    "# 리스트가 있을 때, 각 요소에 3을 곱한 값을 새로운 리스트로 반환하는 \n",
    "number =[1,2,3,4,5]\n",
    "# 람다\n",
    "list(map(lambda x:x*3,number))\n",
    "# 일반\n",
    "def a(x):\n",
    "    return x*3\n",
    "list(map(a,number))\n"
